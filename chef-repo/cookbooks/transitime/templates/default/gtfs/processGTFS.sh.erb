#!/bin/bash

cd /var/lib/oba/transitime/processGTFS
java \
  -Xmx14g \
  -Dtransitime.core.agencyId=<%= node[:transitime][:agency] %> \
  -Dtransitime.logging.dir=/var/lib/oba/transitime/processGTFS \
  -Dtransitime.db.batchSize=16000 \
  -jar \
  GTFSFileProcessor.jar \
  -c /var/lib/oba/transitime/processGTFS/mysql_hibernate.cfg.xml \
  -gtfsDirectoryName /var/lib/oba/transitime/gtfs \
  -storeNewRevs \
  > /var/lib/oba/transitime/processGTFS/gtfs.log 2>&1